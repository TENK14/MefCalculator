<Window x:Class="MefCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MefCalculator"
        mc:Ignorable="d"
        Title="Calculator" Height="200" Width="150">
    <Grid>
        <TextBox x:Name="value1_textBox" HorizontalAlignment="Left" Height="23" Margin="10,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" ToolTip="Value 2"/>
        <TextBox x:Name="value2_textBox" HorizontalAlignment="Left" Height="23" Margin="10,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" ToolTip="Value 2"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="10,76,0,0" VerticalAlignment="Top" Width="120" Height="23" SelectionChanged="comboBox_SelectionChanged" ToolTip="Function"/>
        <TextBox x:Name="result_textBox" HorizontalAlignment="Left" Height="23" Margin="10,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsEnabled="False" ToolTip="Result"/>

    </Grid>
</Window>
